// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Events {
  id String @id @default(uuid())
  name String
  finishTime DateTime
  pin String @unique
  ip String
  createdAt DateTime @default(now())
}

model Members {
  id String @id @default(uuid())
  name String 
  ip String
  eventPin String 
  eventId String
  createdAt DateTime @default(now())
}